{
  "name": "kitzo",
  "version": "2.2.0",
  "description": "A lightweight React micro-utility.",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/react/types.d.ts",
      "import": "./dist/react/index.js",
      "default": "./dist/react/index.js"
    },
    "./fns": {
      "types": "./dist/functions/types.d.ts",
      "import": "./dist/functions/index.js",
      "default": "./dist/functions/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "rollup -c",
    "build-test": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "keywords": [
    "tooltip",
    "toast",
    "hooks",
    "function",
    "micro-utility",
    "ui",
    "reactjs",
    "react",
    "kitzo"
  ],
  "author": "Riyad",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riyad-96/kitzo.git"
  },
  "homepage": "https://github.com/riyad-96/kitzo#readme",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@tailwindcss/vite": "^4.1.17",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "tailwindcss": "^4.1.17",
    "vite": "^7.2.4"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  }
}
